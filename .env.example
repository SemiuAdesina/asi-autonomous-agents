# ASI Autonomous Agents Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://asi_user:asi_password_2024@localhost:5432/asi_agents
DB_HOST=localhost
DB_PORT=5432
DB_NAME=asi_agents
DB_USER=asi_user
DB_PASSWORD=asi_password_2024

# Flask Configuration
FLASK_APP=app.py
FLASK_ENV=development
SECRET_KEY=asi-secret-key-2024-change-in-production
DEBUG=True

# JWT Configuration
JWT_SECRET_KEY=jwt-secret-key-2024-change-in-production
JWT_ACCESS_TOKEN_EXPIRES=3600
JWT_REFRESH_TOKEN_EXPIRES=2592000

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Celery Configuration
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/1

# MeTTa Knowledge Graph Configuration
METTA_SERVER_URL=http://localhost:8080
METTA_API_KEY=metta-api-key-change-in-production

# Security Configuration
BCRYPT_LOG_ROUNDS=12
CORS_ORIGINS=http://localhost:3000,http://localhost:5001

# Rate Limiting
RATELIMIT_STORAGE_URL=redis://localhost:6379/2
RATELIMIT_DEFAULT=1000 per hour

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# Web3 Configuration
WEB3_PROVIDER_URL=https://mainnet.infura.io/v3/your-infura-key
ETHERSCAN_API_KEY=your-etherscan-api-key

# AI/ML Configuration
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
ASI_ONE_API_KEY=your-asi-one-api-key

# Monitoring Configuration
ENABLE_MONITORING=True
METRICS_PORT=9090

# Production Settings (uncomment for production)
# FLASK_ENV=production
# DEBUG=False
# SECRET_KEY=your-production-secret-key
# JWT_SECRET_KEY=your-production-jwt-secret-key
